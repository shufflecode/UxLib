<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!--Standardfarben der Ui Definition-->
    <Color x:Key="CustomBlue">DarkBlue</Color>
    <Color x:Key="CustomRed">DarkRed</Color>
    

    <!--Aufgehellt Blau-->
    <Color x:Key="CustomLight01Blue" >#AA00008B</Color>
    <Color x:Key="CustomLight02Blue" >#8800008B</Color>
    <Color x:Key="CustomLight03Blue" >#6600008B</Color>
    <Color x:Key="CustomLight04Blue" >#4400008B</Color>
    <Color x:Key="CustomLight05Blue" >#2200008B</Color>
    <Color x:Key="BlueHighlightColor">#FF708BBA</Color>
    <Color x:Key="ButtonBorderColor" >#FF121F33</Color>

    <!--Aufgehellt Rot-->
    <Color x:Key="CustomLight01Red">#AAFF0000</Color>
    <Color x:Key="CustomLight02Red">#88FF0000</Color>
    <Color x:Key="CustomLight03Red">#66FF0000</Color>
    <Color x:Key="CustomLight04Red">#44FF0000</Color>
    <Color x:Key="CustomLight05Red">#22FF0000</Color>

    <!--Schriftfarben-->
    <Color x:Key="FontColor" >#ffffffff</Color>
    
</ResourceDictionary>